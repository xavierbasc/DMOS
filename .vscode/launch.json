{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) Debug APP",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/dmos/bin/linux/dm50d",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/dmos/bin/linux/",
            "environment": [],
            "externalConsole": true,
            "MIMode": "lldb",  // Usar lldb
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "debug",
            "logging": {
                "engineLogging": false,  // Desactiva el registro del motor del depurador
                "trace": false,  // Desactiva el rastreo
                "traceResponse": false  // Desactiva las respuestas del rastreo
            }
        }
    ]
}

